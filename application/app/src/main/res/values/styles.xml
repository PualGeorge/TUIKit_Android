<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TRTCLiveConfirmDialogFragment" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>

    <style name="xiaoshipin_AppDialogTheme" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>

    <style name="AppSpinnerStyle">
        <item name="android:textColor">@color/app_color_black</item>
        <item name="android:color">@color/app_color_white</item>
    </style>

    <style name="PopupInputCardAnim">
        <item name="android:windowEnterAnimation">@anim/app_popup_in_anim</item>
        <item name="android:windowExitAnimation">@anim/app_popup_out_anim</item>
    </style>

</resources>